          "alterId": 0,
          "security": "auto",
          "level": 1
        }
      ]
    },
    "streamSettings":{  
      "network":"ws",
      "security":"none",
      "tlsSettings":{
      	"certificates":[{
			"certificateFile":"v2ray.crt",
			"keyFile":"v2ray.key"
		}]
